page{
  background-color: #f5f5f5;
}
.nctouch-default-list-tit{
  font-size: 35rpx;
    line-height: 50rpx;
    padding: 25rpx;
}
.nctouch-default-list{
      overflow: hidden;
    padding: 0 0 0 25rpx;
    background-color: #FFF;
    border-top: solid 2.5rpx #EEE;
    border-bottom: solid 2.5rpx #EEE;
}
.list_item:first-child{
  border-top: none;
}
.list_item{
      position: relative;
    z-index: 1;
    display: block;
    clear: both;
    overflow: hidden;
    padding: 25rpx 0;
    line-height:50rpx;
    border-top: solid 2.5rpx #EEE;
}
.h4{
      float: left;
    margin-left: 15rpx;
    font-size: 35rpx;
    line-height: 50rpx;
    color: #333;
}
.num{
      float: right;
    margin-right: 60rpx;
    font-size: 30rpx;
    line-height: 50rpx;
}
.pics{
  float: right;
    margin-right: 60rpx;
    font-size: 30rpx;
    line-height: 50rpx;
}
.pics image{
  width: 50rpx;
    height: 50rpx!important;
    display: inline-block;
}