.track-list{
    margin:40rpx ;
    padding-left: 10rpx;
    position: relative;
}
.track-list .li.first {
    color: red;
    padding-top: 0;
    border-left-color: #fff;
}
.track-list .li {
    position: relative;
    padding: 30rpx 0 0 50rpx;
    line-height: 36rpx;
    border-left: 2rpx solid #d9d9d9;
    color: #999;
}
.node-icon{
      position: absolute;
    left: -12rpx;
    top: 50%;
    width: 22rpx;
    height: 22rpx;
}
.time{
  margin-right: 40rpx;
    position: relative;
    /* top: 8rpx; */
    display: inline-block;
    vertical-align: middle;
    font-size: 28rpx
}
.text{
  font-size: 28rpx;
      line-height: 50rpx;
    position: relative;
    /* top: 8rpx; */
   
}
page{
  background-color: #f5f5f5;
}
.container{
  background-color: #fff;
  padding: 20rpx;
}
.container>view{
  font-size: 32rpx;
  line-height: 50rpx;
  margin-left: 20rpx;
}