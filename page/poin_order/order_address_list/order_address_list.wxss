.address_list{
  padding: 20rpx;
  font-size: 36rpx;
}
.it_bottom{
  font-size: 28rpx;
  color:#666666;
}
.it_bottom text{
  margin-right: 20rpx;
}
.address_item{
  border-bottom: 1px solid #e4e4e4;
  padding-bottom: 20rpx;
  position: relative;
}
.add_address{
  width: 90%;
  height: 80rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  margin-top: 20rpx;

}
.z{
   color: #fa4e51!important;
}
.b{
  background-color:#fa4e51!important; 
}
.mo{
  position: absolute;
  font-size: 24rpx;
  right: 0;
  padding: 2rpx;
  border: 1px solid #fa4e51;
}
.zhe{
  position:fixed;
  width: 100%;
  height: 100%;
  background-color:rgba( 0,0,0,.3);
  top: 0;
  left: 0;
  z-index: 100

}
.xz{
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 80%;
  margin: 0 auto;
  background-color: #f5f5f5;
  border-radius: 4rpx;
  height: 150rpx;
  padding: 20rpx ;
}
.xz navigator{
    display: block;
    height: 75rpx;
    font-size: 36rpx;
    line-height: 75rpx;
    
}
.xz text{
    display: block;
    height: 75rpx;
    font-size: 36rpx;
    line-height: 75rpx;
    
}
.xz navigator:first-child{
  border-bottom: 2rpx solid #e4e4e4;
}
.xz navigator{
  z-index: 100;
}
.xz text{
  z-index: 100;
}