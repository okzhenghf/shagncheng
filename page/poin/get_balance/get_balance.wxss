.nctouch-asset-info{
  display: block;
}
.voucher{
  background: #FB6E52;
  position: relative;
    z-index: 1;
    height: 150rpx;
}
.voucher image{
  position: absolute;
    z-index: 1;
    top: 0;
    left: 75rpx;
    display: block;
    width: 150rpx;
    height: 150rpx;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    background-size: contain;
    opacity: 0.3;
    -webkit-transform: rotate(-25deg);
    -ms-transform: rotate(25deg);
    transform: rotate(25deg);
}
.dl{
      text-align: left;
          width: 100%;
          padding: 25rpx 0;
}
.dd{
  color: #fff;
  padding-left: 20rpx;
  line-height: 35rpx;
    height: auto;
    font-size: 30rpx;
   

}
.dd:first-child{
  margin-bottom: 10px;
}
.form-item{
  background-color: #fff;
  margin-left: 0;
    margin-bottom: 15rpx;
    border-bottom: 1px solid #e4e4e4;
    border-top: 1px solid #e4e4e4;
    position: relative;
    z-index: 1;
    display: block;
}
.h4{
  color: #444;
    text-align: right;
        position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width:150rpx;
    line-height: 130rpx;
    font-size: 30rpx;
}
.input-box{
      display: block;
    height: 130rpx;
    margin: 0 25rpx 0 175rpx;
    position: relative;
    z-index: 1;
}
input{
  padding: 45rpx 0;
    font-size: 30rpx;
     width: 100%;
    min-height: 47.5rpx;
    line-height: 47.5rpx;
    color: #000;
    border: none;
}
page{
  background-color: #f5f5f5;
}
.txt{
  font-size: 30rpx;
  margin-left: 20rpx;
  position: absolute;
}
.btn{
  margin-top: 70rpx;
}