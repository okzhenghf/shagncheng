.order-list{
  background-color: #f5f5f5;
}
.order_item{
    border-top: 1px solid #e4e4e4;
    overflow: hidden;
    margin-bottom: 13rpx;
    border-bottom: 1px solid #e4e4e4;
    padding: 0 3%;
    background-color: #ffffff;
}
.li_con{
  overflow: hidden;
  padding: 39rpx 0;
}

.order_image{
      width: 140rpx;
    height: 140rpx;
    border: 1px solid #e4e4e4;
    float: left;
}
.order_image image{
  width: 100%!important;
  height: 100% !important;
}
.order_name{
  float: left;
    width: 340rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-size: 30rpx;
    color: #666666;
    margin-left: 26rpx;
    line-height: 55rpx;
}
.order_ran {
    float: right;
}

.order_ran .jif {
    font-size: 31rpx;
   
    line-height: 100rpx;
}

.order_ran view{
  text-align: right;
}

.order_ran>view:last-child {
    font-size: 29rpx;
    color: #999999;
}
.status_order{
  font-size: 25rpx;
    color: #999999;
}



.nctouch-norecord {
    position: absolute;
    z-index: -1;
    top: 50%;
    left: 50%;
    width: 500rpx;
    height: 410rpx;
    margin: -150rpx 0 0 -250rpx;
    text-align: center;
    font-size: 0;
}
.norecord-ico{

  display: inline-block;
    width: 110rpx;
    height: 110rpx;
    padding: 20rpx;
    margin: 0 auto;
    background-color: #DDD;
    border-radius: 100%;
}

.norecord-ico image{
  display: block;
    width: 100%;
    height: 100%;
}
.cord_b {
    height: 137rpx;
    margin-top: 37rpx;
}
.f_cord{
      display: block;
    height: 50rpx;
    margin-bottom: 12rpx;
    font-size: 38rpx;
    line-height:50rpx;
}
.b_cord{
  display: block;
    height: 50rpx;
    margin-bottom: 25rpx;
    font-size: 28rpx;
    line-height: 50rpx;
    color: #999;
}
.nctouch-norecord navigator{

  vertical-align: top;
    display: inline-block !important;
    height: 55rpx;
    padding: 0 15rpx;
    margin: 0 auto;
    font-size: 30rpx;
    color: #555 !important;
    line-height: 55rpx;
    text-align: center;
    background-color: #FFF;
    border: solid 1px #CCC;
    border-radius: 8rpx;
}







